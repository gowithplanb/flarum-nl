{
    "name": "gowithplanb/flarum-de",
    "type": "flarum-extension",
    "description": "Dutch language pack for Flarum and its bundled extensions.",
    "homepage": "http://flarum.org/",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
        "flarum-extension": {
            "title": "Dutch",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "nl",
            "title": "Dutch"
        }
    }
}
